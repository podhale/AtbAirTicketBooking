<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
    <class name="com.podhale.domain.Miejsce" table="miejsce">
    <meta attribute="class-description">
        Klasa zawiera lokalizacje lotnisk.
    </meta>
        <id name="id" type="java.lang.Long" column="ID">
            <generator class="native" />
        </id>
        <property name="name" column="NAME" type="java.lang.String"/>
        <property name="city" column="CITY" type="java.lang.String"/>
        <property name="code" column="CODE" type="java.lang.String"/>
        <property name="country" column="COUNTRY" type="java.lang.String"/>
        

    </class>


</hibernate-mapping>